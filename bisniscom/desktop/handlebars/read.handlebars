        $("#snap-button-new").click(function () {
            var productID = $('input[name="pilih-subs"]:checked').val();
            var URL = 'https://services-apps.bisnis.com/';
            var bisnisID = '';

            $.ajax({
                url: URL + 'subscribe/getTokenSnapNew',
                method: 'POST',
                data: {
                    'bisnisId': bisnisID,
                    'productId': productID
                },
                dataType: "json",
                success: function (data) {
                    snap.pay(data.snapToken, {
                        onSuccess: function (result) {
                            var grossAmount = result.gross_amount;
                            var price = grossAmount.substr(0, grossAmount.length - 3);

                            $.ajax({
                                url: URL + 'subscribe/insertTransactionNew',
                                method: 'POST',
                                data: {
                                    'bisnis_id': bisnisID,
                                    'order_code': result.order_id,
                                    'totalOrder': price,
                                    'dateOrder': result.transaction_time,
                                    'transaction_status': result.transaction_status,
                                    'payment_method': result.payment_type,
                                    'product_id': productID,
                                    'fraud_status': result.fraud_status
                                },
                                dataType: "json",
                                beforeSend: function () { },
                                success: function (data, textStatus, xhr) {
                                    if (productID == '7250') {
                                        $('.txt')[0].innerHTML =
                                            '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (1 Bulan) Reguler';
                                    } else if (productID == '7253') {
                                        $('.txt')[0].innerHTML =
                                            '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (3 Bulan) Reguler';
                                    } else if (productID == '7254') {
                                        $('.txt')[0].innerHTML =
                                            '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (6 Bulan) Reguler';
                                    } else if (productID == '7255') {
                                        $('.txt')[0].innerHTML =
                                            '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (12 Bulan) Reguler';
                                    } else {
                                        $('.txt')[0].innerHTML =
                                            '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content 1 Minggu';
                                    }
                                    $('#premium-popup').modal('show');
                                },
                                error: function (e) {
                                    $('#img-premium-popup')[0].src =
                                        'https://static.bisnis-cdn.com/bisnis/assets/images/2020/notif-premium-failed.svg';
                                    $('.txt')[0].innerHTML =
                                        '<strong>Transaksi Gagal!</strong><br />Harap lakukan transaksi beberapa saat lagi';
                                    $('#premium-popup').modal('show');
                                }
                            });
                        },
                        onPending: function (result) {
                            var grossAmount = result.gross_amount;
                            var price = grossAmount.substr(0, grossAmount.length - 3);

                            $.ajax({
                                url: URL + 'subscribe/insertTransactionNew',
                                method: 'POST',
                                data: {
                                    'bisnis_id': bisnisID,
                                    'order_code': result.order_id,
                                    'totalOrder': price,
                                    'dateOrder': result.transaction_time,
                                    'transaction_status': result.transaction_status,
                                    'payment_method': result.payment_type,
                                    'product_id': productID,
                                    'fraud_status': result.fraud_status
                                },
                                dataType: "json",
                                beforeSend: function () { },
                                success: function (data, textStatus, xhr) {
                                    if (data.trans_status == 'pending') {
                                        if (result.payment_type == 'bank_transfer' || result.payment_type == 'echannel') {
                                            /* Condition for Mandiri VA */
                                            if (result.payment_type == 'echannel') {
                                                var bankName = 'Bank Mandiri';
                                                var billerCode = result.biller_code;
                                                var VAnumber = result.bill_key;
                                            } else {
                                                /* Condition for BNI or Other Bank with BNI VA */
                                                if (result.va_numbers[0].bank == 'bni') {
                                                    var bankName = 'Bank BNI';
                                                    var billerCode = '';
                                                    var VAnumber = result.va_numbers[0].va_number;
                                                }
                                                /* Condition for BRI VA */
                                                else if (result.va_numbers[0].bank == 'bri') {
                                                    var bankName = 'Bank BRI';
                                                    var billerCode = '';
                                                    var VAnumber = result.va_numbers[0].va_number;
                                                } else {
                                                    /* Condition for Permata VA*/
                                                    var bankName = 'Bank Permata';
                                                    var billerCode = '';
                                                    var VAnumber = result.permata_va_number;
                                                }
                                            }

                                            $('.txt')[0].innerHTML =
                                                'Segera lakukan pembayaran Anda ke:<br /><strong>' +
                                                bankName +
                                                '</strong> dengan nomor Virtual Account <strong>' +
                                                billerCode + VAnumber +
                                                '</strong>';
                                            $('#premium-popup').modal('show');
                                        } else {
                                            $('#img-premium-popup')[0].src =
                                                'https://static.bisnis-cdn.com/bisnis/assets/images/2020/notif-premium-failed.svg';
                                            $('.txt')[0].innerHTML =
                                                '<strong>Transaksi Gagal!</strong><br />Harap lakukan transaksi beberapa saat lagi';
                                            $('#premium-popup').modal('show');
                                        }
                                    } else if (data.trans_status ==
                                        'settlement') {
                                        if (result.payment_type ==
                                            'bank_transfer' || result
                                                .payment_type == 'echannel') {
                                            if (productID == '7250') {
                                                $('.txt')[0].innerHTML = '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (1 Bulan) Reguler';
                                            } else if (productID == '7253') {
                                                $('.txt')[0].innerHTML = '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (3 Bulan) Reguler';
                                            } else if (productID == '7254') {
                                                $('.txt')[0].innerHTML = '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (6 Bulan) Reguler';
                                            } else if (productID == '7255') {
                                                $('.txt')[0].innerHTML = '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content Basic (12 Bulan) Reguler';
                                            } else {
                                                $('.txt')[0].innerHTML = '<strong>Transaksi Berhasil!</strong><br />Selamat, Anda Telah Berlangganan Paket Premium Content 1 Minggu';
                                            }
                                            $('#premium-popup').modal('show');
                                        }
                                    }

                                },
                                error: function (e, data) {
                                    $('#img-premium-popup')[0].src =
                                        'https://static.bisnis-cdn.com/bisnis/assets/images/2020/notif-premium-failed.svg';
                                    $('.txt')[0].innerHTML =
                                        '<strong>Transaksi Gagal!</strong><br />Harap lakukan transaksi beberapa saat lagi';
                                    $('#premium-popup').modal('show');
                                }
                            });
                        },
                        onError: function (result) {
                            $('#img-premium-popup')[0].src =
                                'https://static.bisnis-cdn.com/bisnis/assets/images/2020/notif-premium-failed.svg';
                            $('.txt')[0].innerHTML =
                                '<strong>Transaksi Gagal!</strong><br />Harap lakukan transaksi beberapa saat lagi';
                            $('#premium-popup').modal('show');
                        }
                    });
                }
            });
        });

