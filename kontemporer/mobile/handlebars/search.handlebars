                <li class="text-center"><a href="#" class="searchType__item buttonPrimary">Video</a></li>
                <li class="text-center"><a href="#" class="searchType__item buttonPrimary">Komentar</a></li>
                <li class="text-center"><a href="#" class="searchType__item buttonPrimary">User</a></li>
            </ul>

            {{> article mt="30" index_start=0 index_end=5}}

            <div class="flex-center mt10">
                <button class="button buttonMore">Load More</a>
            </div>
        </section>

    </main>
    {{!> ads/sticky}}

    <!-- footer -->
    {{> footer}}

    {{> js}}

