{{#each trending}}
{{#gte @index compare=../index_start}}
{{#gte @index compare=../index_end}}
{{else}}
<li class="articleItem mt20 display-flex">
    <a href="{{link}}" class="articleImg__wrap" aria-label="{{title}}">
        <img src="{{img}}{{@index}}.jpg" class="articleImg" alt="" width="178" height="155" loading="lazy">
    </a>
    <div class="articleBox">
        {{#if ../sub}}
        <div class="articleSubtitle">{{subtitle}}</div>
        {{/if}}
        {{#if ../tag}}
        <{{.. /tag}} class="articleTitle">
            <a href="{{link}}" class="articleLink">
                {{title}}
            </a>
        </{{.. /tag}}>
        {{else}}
        <div class="articleTitle">
            <a href="{{link}}" class="articleLink">
                {{title}}
            </a>
        </div>
        {{/if}}
        {{#if ../time}}
        <div class="articleTime mt10">Minggu, 18 Mei 2025</div>
        {{/if}}
        {{#if ../lead}}
        <div class="articleLead">{{lead}}</div>
        {{/if}}
        {{#if ../next}}<div class="buttonNext">Selanjutnya <span class="icon icon-next"></span></div>{{/if}}
        {{#if ../timebtm}}
        <div class="articleDetail display-flex aitems-center mt10">
            {{#if ../user}}
            <a href="#" class="display-flex aitems-center">
                <img loading="lazy" src="{{user}}" width="24" height="24" alt="" class="userAvatar mr10">
                <div class="userName">
                    Salma Farida
                </div>
            </a>
            {{/if}}
            <div class="articleTime">Minggu, 18 Mei 2025</div>
        </div>
        {{/if}}
    </div>
</li>
{{/gte}}
{{else}}
{{/gte}}
{{/each}}