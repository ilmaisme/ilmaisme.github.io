<!DOCTYPE html>
<html lang="en">
{{> head page="Home"}}

<body>

	<!-- header -->
	{{> header page="Home" login=true}}
	<!-- header -->

	<!-- main -->
	<main role="main">
		{{> ads/skin}}
		{{> ads/billboard}}
		<section class="container">
			<div class="row display-flex mt40">
				<div class="col-gd-740">
					{{> headline}}
					{{> highlight}}
					{{> ads/middle}}
					{{> highlight-video}}
					{{> article}}
					{{> ads/middle}}
					{{> highlight-stories}}
					{{> article}}
					{{> highlight-photo}}
					{{> article}}
					{{> ads/parallax}}
					{{> highlight-box}}
					{{> article-more}}
				</div>
				<div class="col-gd-300 -parent-sticky">
					<aside>
						{{> ads/right}}
						{{> ads/right mt=20}}
						{{> sidebar/trending}}
						{{> ads/halfpage}}
						{{> sidebar/recomm}}
						{{> sidebar/hashtag}}
						{{> sidebar/highlight}}
						{{> ads/right-sticky}}
					</aside>
				</div>
			</div>
		</section>
	</main>
	{{> ads/sticky}}
	<!-- footer -->
	{{> footer}}

	{{> js page="Home" json=true slick=true}}

</body>

</html>