<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    {{#each channel}}
    <title>{{name}}</title>
    <meta name="description" content="{{description}}">
    <meta name="keywords" content="{{keywords}}">

    <meta property="og:type" content="article" />
    <meta property="og:url" content="" />
    <meta property="og:title" content="{{name}}" />
    <meta property="og:description" content="{{description}}" />

    <meta property="og:image" content="" />
    <meta property="og:site_name" content="" />
    <meta property="fb:app_id" content="" />
    <!-- s:twitter card -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="" />
    <meta name="twitter:creator" content="">
    <meta name="twitter:title" content="{{name}}" />
    <meta name="twitter:description" content="{{description}}" />
    <meta name="twitter:image" content="" />
    <!-- e:twitter card -->
    {{/each}}

    <link rel=icon type=image/png sizes=16x16 href="https://cdn.bisnis.com/bisnis/assets/images/favicon.png">
    {{#if slick}}
    <!-- slick CSS -->
    <link rel="stylesheet" href="./asset/css/slick.min.css" type="text/css">
    {{/if}}

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    {{#eq page compare="Lifestyle"}}
    {{#eq type compare="details"}}
    <link rel="preload"
        href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,400;0,600;0,700;1,600;1,700&display=swap"
        as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript>
        <link rel="stylesheet"
            href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,400;0,600;0,700;1,600;1,700&display=swap">
    </noscript>
    {{#if premium}}
    <link rel="stylesheet" href="./asset/css/style-lifestyle-details-premium.min.css" type="text/css">
    {{else}}
    <link rel="stylesheet" href="./asset/css/style-lifestyle-details.min.css" type="text/css">
    {{/if}}
    {{else}}
    <link rel="preload"
        href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,400;0,600;0,700;1,700&display=swap"
        as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript>
        <link rel="stylesheet"
            href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,400;0,600;0,700;1,700&display=swap">
    </noscript>
    <link rel="stylesheet" href="./asset/css/style-lifestyle.min.css" type="text/css">
    {{/eq}}
    {{#if hl}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/posts/2023/01/25/1621417/dungeons--dragons-honor-among-thieves.jpg">
    {{/if}}
    {{#if latest}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/04/05/1643981/diabetes_1668425885.jpg?w=369&h=277">
    {{/if}}
    {{/eq}}
    {{#eq page compare="Tekno"}}
    {{#eq type compare="details"}}
    <link rel="preload"
        href="https://fonts.googleapis.com/css2?family=Prompt:ital,wght@0,300;0,400;0,500;0,600;1,400;1,600&display=swap"
        as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript>
        <link rel="stylesheet"
            href="https://fonts.googleapis.com/css2?family=Prompt:ital,wght@0,300;0,400;0,500;0,600;1,400;1,600&display=swap">
    </noscript>
    {{#if premium}}
    <link rel="stylesheet" href="./asset/css/style-tekno-details-premium.min.css" type="text/css">
    {{else}}
    <link rel="stylesheet" href="./asset/css/style-tekno-details.min.css" type="text/css">
    {{/if}}
    {{else}}
    <link rel="preload"
        href="https://fonts.googleapis.com/css2?family=Prompt:ital,wght@0,300;0,400;0,600;1,600&display=swap"
        as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript>
        <link rel="stylesheet"
            href="https://fonts.googleapis.com/css2?family=Prompt:ital,wght@0,300;0,400;0,600;1,600&display=swap">
    </noscript>
    <link rel="stylesheet" href="./asset/css/style-tekno.min.css" type="text/css">
    {{/eq}}
    {{#if hl}}
    <link rel="preload" as="image" href="https://images.bisnis.com/posts/2022/11/21/1600267/twitter-.jpg">
    {{/if}}
    {{#if latest}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/03/30/1642192/cyber_security.jpg?w=740&h=393">
    {{/if}}
    {{/eq}}
    {{#if photozoom}}
    <link rel="stylesheet" type="text/css" href="./asset/css/photoswipe/photoswipe.min.css">
    {{/if}}

    <!--font menu & footer -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;700&display=swap">
    <link rel="preload" href="./asset/images/logo/{{replace (lowercase page)}}-logo-m.svg" as="image" />
</head>