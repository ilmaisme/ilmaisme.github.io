<div class="art">
    <div class="artWrap -js-photo-slider">
        {{#each channel}}
        {{#gte @index compare=../index_start}}
        {{#gte @index compare=../index_end}}
        {{else}}
        <div class="artItem">
            <div class="artImg">
                <img src="https://images.bisnis.com/thumb/posts/{{img}}?w={{../w}}&h={{../h}}" width="{{../w}}"
                    height="{{../h}}" alt="{{title}}" loading="lazy">
            </div>
            <div class="artContent">
                <div class="artPhotoCount">
                    <span class="icoInline icoPhoto"></span>5 Foto
                </div>
                <div class="artContentWrap">
                    <div class="artChannel">
                        <a href="#">
                            {{channel}}
                        </a>
                    </div>
                    <div class="artDate">
                        9 menit yang lalu
                    </div>
                </div>
                <a href="#" class="artLink">
                    <h4 class="artTitle">
                        {{title}}
                    </h4>
                </a>
                <a href="#" class="artPhotoMore">Lihat Foto</a>
            </div>
        </div>
        {{/gte}}
        {{else}}
        {{/gte}}
        {{/each}}
    </div>
    <div class="artPhotoNav -js-photo-slider-nav">
        {{#each channel}}
        {{#gte @index compare=../index_start}}
        {{#gte @index compare=../index_end}}
        {{else}}
        <div class="artPhotoNavItem">
            <div class="artPhotoNavWrap">
                <div class="artPhotoNavImg">
                    <img src="https://images.bisnis.com/thumb/posts/{{img}}?w={{../w}}&h={{../h}}" width="{{../w}}"
                        height="{{../h}}" alt="{{title}}" loading="lazy">
                </div>
                <div class="artPhotoNavContent">
                    <div class="artContentWrap">
                        <div class="artChannel">
                            <a href="#">
                                {{channel}}
                            </a>
                        </div>
                        <div class="artDate">
                            9 menit yang lalu
                        </div>
                    </div>
                    <a href="#" class="artLink">
                        <h4 class="artTitle">
                            {{title}}
                        </h4>
                    </a>
                </div>
            </div>
        </div>
        {{/gte}}
        {{else}}
        {{/gte}}
        {{/each}}
    </div>
</div>