<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    {{#each channel}}
    <title>{{name}}</title>
    <meta name="description" content="{{description}}">
    <meta name="keywords" content="{{keywords}}">

    <meta property="og:type" content="article" />
    <meta property="og:url" content="" />
    <meta property="og:title" content="{{name}}" />
    <meta property="og:description" content="{{description}}" />

    <meta property="og:image" content="" />
    <meta property="og:site_name" content="" />
    <meta property="fb:app_id" content="" />
    <!-- s:twitter card -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="" />
    <meta name="twitter:creator" content="">
    <meta name="twitter:title" content="{{name}}" />
    <meta name="twitter:description" content="{{description}}" />
    <meta name="twitter:image" content="" />
    <!-- e:twitter card -->
    {{/each}}

    <link rel=icon type=image/png sizes=16x16 href="https://cdn.bisnis.com/bisnis/assets/images/favicon-circle.ico">
    {{#if slick}}
    <!-- slick CSS -->
    <link rel="stylesheet" href="./asset/css/slick.min.css" type="text/css">
    {{/if}}

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    {{#eq page compare="Lifestyle"}}
    {{#eq type compare="details"}}
    <link
        href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,400;0,600;0,700;1,600;1,700&display=swap"
        rel="stylesheet">
    {{#if premium}}
    <link rel="stylesheet" href="./asset/css/style-lifestyle-details-premium.css" type="text/css">
    {{else}}
    <link rel="stylesheet" href="./asset/css/style-lifestyle-details.css" type="text/css">
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/01/25/1621417/dungeons--dragons-honor-among-thieves.jpg?w=1080&h=378">
    {{/if}}
    {{else}}
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,400;0,600;0,700;1,700&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="./asset/css/style-lifestyle.css" type="text/css">
    {{/eq}}
    {{#if hl}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/posts/2023/01/25/1621417/dungeons--dragons-honor-among-thieves.jpg">
    {{/if}}
    {{#if latest}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/04/05/1643981/diabetes_1668425885.jpg?w=369&h=277">
    {{/if}}
    {{/eq}}
    {{#eq page compare="Tekno"}}
    {{#eq type compare="details"}}
    <link
        href="https://fonts.googleapis.com/css2?family=Prompt:ital,wght@0,300;0,400;0,500;0,600;1,400;1,600&display=swap"
        rel="stylesheet">
    {{#if premium}}
    <link rel="stylesheet" href="./asset/css/style-tekno-details-premium.css" type="text/css">
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/03/30/1642230/hari-film-nasional-premium.jpg?w=1440&h=840">
    {{else}}
    <link rel="stylesheet" href="./asset/css/style-tekno-details.css" type="text/css">
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2022/11/21/1600267/twitter-.jpg?w=740&amp;h=420">
    {{/if}}
    {{else}}
    <link href="https://fonts.googleapis.com/css2?family=Prompt:ital,wght@0,300;0,400;0,600;1,600&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="./asset/css/style-tekno.css" type="text/css">
    {{/eq}}
    {{#if hl}}
    <link rel="preload" as="image" href="https://images.bisnis.com/posts/2022/11/21/1600267/twitter-.jpg">
    {{/if}}
    {{#if latest}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/03/30/1642192/cyber_security.jpg?w=740&h=393">
    {{/if}}
    {{/eq}}
    {{#eq page compare="Ekonomi"}}
    {{#eq type compare="details"}}
    {{#if premium}}
    <link rel="stylesheet" href="./asset/css/style-ekonomi-details-premium.min.css" type="text/css">
    {{else}}
    <link rel="stylesheet" href="./asset/css/style-ekonomi-details.min.css" type="text/css">
    {{/if}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/posts/2023/06/05/1662334/14032023-bi-dul-23-komoditas_pangan-2_1680516502.jpg">
    {{else}}
    <link rel="stylesheet" href="./asset/css/style-ekonomi.min.css" type="text/css">
    {{/eq}}
    <link href="https://fonts.googleapis.com/css2?family=Manrope:wght@300;600&display=swap" rel="stylesheet">
    {{#if hl}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/06/19/1666608/mpv-shot0004-1-1.jpg?w=484&h=270">
    {{/if}}
    {{#if latest}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/06/20/1667092/20122022-bi-hil-21-garuda-5.jpg?w=348&h=196">
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2023/06/19/1667034/phe_1681464108.jpg?w=348&h=196">
    {{/if}}
    {{/eq}}
    {{#eq page compare="Homepage"}}
    <link rel="stylesheet" href="./asset/css/style-home.min.css" type="text/css">
    <link rel="preload" href="./asset/images/logo/bisniscom-logo.svg" as="image" />
    {{!--
    <link href="https://fonts.googleapis.com/css2?family=Manrope:wght@300;600&display=swap" rel="stylesheet"> --}}
    <style>
        @font-face {
            font-family: 'Manrope';
            font-style: normal;
            font-weight: 300;
            font-display: swap;
            src: local('Manrope Light'), local('Manrope-Light'), url(https://fonts.gstatic.com/s/manrope/v15/xn7_YHE41ni1AdIRqAuZuw1Bx9mbZk6jFN_C-bk.woff2) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
        }

        @font-face {
            font-family: 'Manrope';
            font-style: normal;
            font-weight: 600;
            font-display: swap;
            src: local('Manrope SemiBold'), local('Manrope-SemiBold'), url(https://fonts.gstatic.com/s/manrope/v15/xn7_YHE41ni1AdIRqAuZuw1Bx9mbZk4jE9_C-bk.woff2) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
        }
    </style>
    {{#if hl}}
    <link rel="preload" as="image"
        href="https://images.bisnis.com/thumb/posts/2024/06/28/1777779/peluncuran_logo_baru_bank_tabungan_negara_bbtn_1709462290.jpg?w=728&h=406">
    {{/if}}
    {{/eq}}
    {{#if photozoom}}
    <link rel="stylesheet" type="text/css" href="./asset/css/photoswipe/photoswipe.min.css">
    {{/if}}

    <!--font menu & footer -->
    <link rel="preload" href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;700&display=swap" as="style"
        onload="this.onload=null;this.rel='stylesheet'">
    <noscript>
        <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;700&display=swap">
    </noscript>
    {{#if font}}
    {{else}}
    <link rel="preload" href="./asset/images/logo/{{replace (lowercase page)}}-logo.svg" as="image" />
    {{/if}}
</head>