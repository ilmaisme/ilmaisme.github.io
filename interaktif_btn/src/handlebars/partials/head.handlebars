    <style>
        @font-face {
            font-family: 'Poppins';
            font-style: italic;
            font-weight: 400;
            font-display: swap;
            src: local('Poppins Italic'), local('Poppins Regular Italic'), url(https://fonts.gstatic.com/s/poppins/v22/pxiGyp8kv8JHgFVrJJLucHtAOvWDSA.woff2) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
        }

        @font-face {
            font-family: 'Poppins';
            font-style: normal;
            font-weight: 400;
            font-display: swap;
            src: local('Poppins'), local('Poppins Regular'), url(https://fonts.gstatic.com/s/poppins/v22/pxiEyp8kv8JHgFVrJJfecnFHGPc.woff2) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
        }

        @font-face {
            font-family: 'Poppins Bold';
            font-style: normal;
            font-weight: 700;
            font-display: swap;
            src: local('Poppins Bold'), local('Poppins-Bold'), url(https://fonts.gstatic.com/s/poppins/v22/pxiByp8kv8JHgFVrLCz7Z1xlFd2JQEk.woff2) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
        }

        @font-face {
            font-family: 'Tomorrow Medium';
            font-style: normal;
            font-weight: 500;
            font-display: swap;
            src: local('Tomorrow Medium'), local('Tomorrow-Medium'), url('./asset/fonts/Tomorrow-Medium.woff2') format('woff2'), url('./asset/fonts/Tomorrow-Medium.woff') format('woff'), url('./asset/fonts/Tomorrow-Medium.ttf') format('truetype')
        }

        @font-face {
            font-family: 'Tomorrow Medium';
            font-style: italic;
            font-weight: 500;
            font-display: swap;
            src: local('Tomorrow MediumItalic'), local('Tomorrow-Medium-Italic'), url('./asset/fonts/Tomorrow-MediumItalic.woff2') format('woff2'), url('./asset/fonts/Tomorrow-MediumItalic.woff') format('woff'), url('./asset/fonts/Tomorrow-MediumItalic.ttf') format('truetype')
        }

        @font-face {
            font-family: 'Tomorrow SemiBold';
            font-style: normal;
            font-weight: 600;
            font-display: swap;
            src: local('Tomorrow SemiBold'), local('Tomorrow-SemiBold'), url('./asset/fonts/Tomorrow-SemiBold.woff2') format('woff2'), url('./asset/fonts/Tomorrow-SemiBold.woff') format('woff'), url('./asset/fonts/Tomorrow-SemiBold.ttf') format('truetype')
        }

        @font-face {
            font-family: 'Tomorrow ExtraBold';
            font-style: normal;
            font-weight: 800;
            font-display: swap;
            src: local('Tomorrow ExtraBold'), local('Tomorrow-ExtraBold'), url('./asset/fonts/Tomorrow-ExtraBold.woff2') format('woff2'), url('./asset/fonts/Tomorrow-ExtraBold.woff') format('woff'), url('./asset/fonts/Tomorrow-ExtraBold.ttf') format('truetype')
        }

        /*preloader */
        .preloader{top:0;height:100%;width:100%;z-index:1001;position:fixed;overflow:hidden;background:#125efa;background:linear-gradient(180deg,#125efa,#0ebfc5)}.preloader,.preloaderWrap{display:flex;justify-content:center}.preloaderWrap{align-items:center;flex-direction:column;position:relative;opacity:1;transition-property:opacity;transition-timing-function:cubic-bezier(.4,0,.2,1);transition-duration:.7s}.preloaderImg{max-width:32px}@media (min-width:768px){.preloaderImg{max-width:52px}}.preloaderTxt{margin-top:40px;font-family:Poppins Bold,serif;font-size:16px;font-weight:700;line-height:normal;color:#fff}.preloader.disactive .preloaderWrap{opacity:0}.spinner{--d:40px;position:absolute;margin-bottom:68px;width:8px;height:8px;border-radius:50%;color:#f5f038;box-shadow:calc(1*var(--d)) calc(0*var(--d)) 0 0,calc(0.707*var(--d)) calc(0.707*var(--d)) 0 1px,calc(0*var(--d)) calc(1*var(--d)) 0 2px,calc(-0.707*var(--d)) calc(0.707*var(--d)) 0 3px,calc(-1*var(--d)) calc(0*var(--d)) 0 4px,calc(-0.707*var(--d)) calc(-0.707*var(--d)) 0 5px,calc(0*var(--d)) calc(-1*var(--d)) 0 6px;animation:l27 1s infinite steps(8)}@media (min-width:768px){.spinner{--d:50px;width:12px;height:12px}}@keyframes l27{to{transform:rotate(1turn)}}
        /*preloader*/
    </style>

