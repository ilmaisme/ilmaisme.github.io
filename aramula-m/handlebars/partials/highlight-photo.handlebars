<div class="highlight highlight--col2 mt30">
    <div class="display-flex aitems-center jcontent-between">
        <div class="articleCaption">Foto</div>
        <a href="#" class="highlightMore display-flex aitems-center">Lihat Lainnya <span
                class="icon icon-next ml10"></span></a>
    </div>
    <div class="highlightList display-flex row mt10">
        {{#eachIndex headline}}
        {{#gt index compare = '0'}}
        <div class="highlightItem">
            <a href="{{item.link}}" class="highlightImg__wrap" aria-label="{{item.title}}">
                <img src="{{item.img}}{{index}}.jpg" class="highlightImg" alt="" width="152" height="113"
                    loading="lazy">
                <div class="photoCount display-flex aitems-center"><span class="icon icon-photo mr10"></span>5 foto
                </div>
            </a>
            <div class="highlightContent">
                <div class="articleDetail display-flex aitems-center mt10">
                    <div class="articleSubtitle">Foto</div>
                    <div class="articleTime">9 menit yang lalu</div>
                </div>
                <h2 class="articleTitle mt5"><a href="{{item.link}}" class="articleLink">{{item.title}}</a></h2>
            </div>
        </div>
        {{/gt}}
        {{/eachIndex}}
    </div>
    <div class="highlightBottom">
        <div class="highlightDots" id="dtphoto">
        </div>
    </div>
</div>