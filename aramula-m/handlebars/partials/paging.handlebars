<div class="paging display-flex jcontent-center{{#eq mt compare=10}} mt10{{/eq}}{{#eq mt compare=30}} mt30{{/eq}}">
    <div class="pagingItem">
        <a href="#" class="pagingLink pagingLink__prev{{#eq active compare=all}}{{else}} inactive{{/eq}}" aria-label="First Paging"><span
                class="icon icon-angle-double"></span></a>
    </div>
    <div class="pagingItem">
        <a href="#" class="pagingLink pagingLink__prev{{#eq active compare=all}}{{else}} inactive{{/eq}}" aria-label="Prev Paging"><span class="icon icon-angle"></span></a>
    </div>
    <div class="pagingItem">
        <a href="#" class="pagingLink{{#eq active compare=all}}{{else}} active{{/eq}}">1</a>
    </div>
    <div class="pagingItem">
        <a href="#" class="pagingLink">2</a>
    </div>
    <div class="pagingItem">
        <a href="#" class="pagingLink{{#eq active compare=all}} active{{else}}{{/eq}}">3</a>
