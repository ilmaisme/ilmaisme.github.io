    {{> js page="Read" slick=true photozoom=true}}
    <script>
        // photo slider
        $(document).ready(function () {
            // slick carousel
            $('.js-photo-slider-for').slick({
                slidesToShow: 1,
                slidesToScroll: 1,
                arrows: 0,
                fade: !0,
                asNavFor: '.js-photo-slider-nav'
            });
            $('.js-photo-slider-nav').slick({
                dots: 0,
                arrows: !0,
                slidesToShow: 3,
                slidesToScroll: 1,
                infinite: !0,
                speed: 1000,
                autoplay: !0,
                asNavFor: '.js-photo-slider-for',
                focusOnSelect: true
            });
        });
    </script>

</body>

</html>