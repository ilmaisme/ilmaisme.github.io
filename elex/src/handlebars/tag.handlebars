<!DOCTYPE html>
<html lang="en">

{{> head page=""}}

<body>
    {{!> header/header login=true}}
    {{> header/header}}
    <main class="main category --health" role="main">

        <div class="container">

            {{>breadcrumb parent="Tags"}}

        </div>
        <div class="lead --small">
            <div class="leadBg artBg">
                <img class="--left" src="./asset/images/logo-symbol.svg" alt="" width="225" height="155">
            </div>
            <div class="container">
                <div class="leadContent col-xl-10 mx-auto">
                    <div class="titleH4">Tags</div>
                    <h1 class="leadCaption">Komik Petualangan</h1>
                </div>
            </div>
        </div>
        <div class="container">

            <div class="row">
                <div class="leftbar col-xl-9 mx-auto">
                    <div class="artList artRow mt40">

                        {{>article channel=article.Komik index_start=0 index_end=5 lazy=true tag="h3" width="184"
                        height="110"}}

                    </div>
                    <div class="titleWrap mt40">
                        <h2 class="title">Photo</h2>
                        <a href="#" class="titleLink">All Photos</a>
                    </div>

                    {{!>media/photo-widget}}

                    <div class="artList artRow mt40">

                        {{>article channel=article.Quanta index_start=0 index_end=5 lazy=true tag="h3" width="184"
                        height="110"}}

                    </div>
                    <div class="titleWrap mt40">
                        <h2 class="title">Video</h2>
                        <a href="#" class="titleLink">All Videos</a>
                    </div>

                    {{!>media/video-widget}}

                    <div class="artList artRow mt40">

                        {{>article channel=article.Kids index_start=0 index_end=5 lazy=true tag="h3" width="184"
                        height="110"}}

                    </div>

                    {{>paging}}

                </div>
            </div>
        </div>
    </main>
    {{> footer/footer}}

    {{> js}}

</body>

</html>