{{#each channel}}
{{#gte @index compare=../index_start}}
{{#gte @index compare=../index_end}}
{{else}}
<div class="artItem{{#if ../col}} {{../col}}{{/if}}{{#if ../type}} {{../type}}{{/if}}">
    <a href="{{link}}" class="artLink">
        <div class="artImg">
            <img src="{{image}}" alt="{{title}}" width="{{../width}}" height="{{../height}}" {{#if ../lazy}} loading="lazy" {{/if}}>
        </div>
        <div class="artContent">
            {{#if label}}
            <div class="artLabel"> {{label}}</div>
            {{/if}}
            {{#if ../tag}}
            <{{.. /tag}} class="artTitle {{#if ../widget}}titleH4{{else}}titleH3{{/if}}">{{title}}</{{.. /tag}}>
            {{else}}
            <div class="artTitle {{#if ../widget}}titleH4{{else}}titleH3{{/if}}">{{title}}</div>
            {{/if}}
            <div class="artDates"><span class="ico icoCal"></span>{{publish}}</div>
        </div>
    </a>
</div>
{{/gte}}
{{else}}
{{/gte}}
{{/each}}