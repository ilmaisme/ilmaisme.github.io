{{#each channel}}
{{#gte @index compare=../index_start}}
{{#gte @index compare=../index_end}}
{{else}}
<div class="eventItem{{#if ../col}} {{../col}}{{/if}}">
    <div class="eventLink">
        <a href="{{link}}" class="eventImg">
            <img src="{{image}}" alt="" width="240" height="240" {{#if ../lazy}} loading="lazy" {{/if}}>
        </a>
        <div class="eventContent">
            {{#if ../tag}}
            <{{.. /tag}} class="eventTitle titleH4"><a href="{{link}}">{{title}}</a></{{.. /tag}}>
            {{else}}
            <a href="{{link}}" class="eventTitle titleH4">{{title}}</a>
            {{/if}}
            {{#if label}}
            <div class="eventLabel">
                <span class="eventCat"><a href="{{linklanding}}">{{label}}</a></span>
                <span
                    class="eventTag{{#eq tag compare='Past Event'}} -past{{/eq}}{{#eq tag compare='On Going'}} -going{{/eq}}">{{tag}}</span>
            </div>
            {{/if}}
            {{#if ../lead}}
            <div class="eventLead">{{lead}}</div>
            <div class="eventAttr eventDates"><span class="ico icoCal"></span>{{dates}}</div>
            <div class="eventAttr eventLoc"><span class="ico icoLoc"></span>{{loc}}</div>
            {{else}}
            <div class="eventAttr eventDates"><span class="ico icoCal"></span>{{dates}}</div>
            {{/if}}
        </div>
    </div>
</div>
{{/gte}}
{{else}}
{{/gte}}
{{/each}}