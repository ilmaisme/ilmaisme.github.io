<div class="paging">
    <form action="#" class="pagingForm{{#eq center compare=false}}{{else}} center-flex{{/eq}}">
        <input type="text" class="pagingInput text-center" name="paging" id="paging" inputmode="numeric" value="1">
        <div class="pagingPage">dari 20 halaman</div>
        <button class="button pagingCta --prev disabled" type="button" aria-label="Previous page">
            <span class="ico icoChevron --prev"></span>
        </button>
        <button class="button pagingCta --next" type="button" aria-label="Next page">
            <span class="ico icoChevron --next"></span>
        </button>
    </form>
</div>