{{#each collab.Team}}
<div class="collabTeam__item col">
    <div class="aboutImg">
        <img src="./asset/images/dummy/collab/member-{{add @index 1}}.jpg" alt="" width="162" height="220"
            loading="lazy">
    </div>
    <div class="collabTeam__title collabServices__title">{{name}}</div>
    <div class="collabServices__content">{{title}}</div>
    <a href="#" class="collabCall center-flex" target="_blank" rel="noopener noreferrer">
        <span class="ico icoWhatsapp"></span>
        Call me
    </a>
</div>
{{/each}}