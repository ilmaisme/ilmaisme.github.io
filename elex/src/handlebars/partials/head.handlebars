    <style>
        @font-face {
            font-family: 'Author-Semibold';
            src: local('Author Semibold'), local('Author-Semibold'),
                url('./asset/fonts/Author-Semibold.woff2') format('woff2'),
                url('./asset/fonts/Author-Semibold.woff') format('woff'),
                url('./asset/fonts/Author-Semibold.ttf') format('truetype');
            font-weight: 400;
            font-display: swap;
            font-style: normal;
        }
        @font-face {
            font-family: 'Author-Semibold';
            src: local('Author Semibold'), local('Author-Semibold'),
                url('./asset/fonts/Author-SemiboldItalic.woff2') format('woff2'),
                url('./asset/fonts/Author-SemiboldItalic.woff') format('woff'),
                url('./asset/fonts/Author-SemiboldItalic.ttf') format('truetype');
            font-weight: 400;
            font-display: swap;
            font-style: italic;
        }
    </style>
    {{/eq}}
    <link rel="preload" href="./asset/images/logo-elex-sm.svg" as="image" />
