<!DOCTYPE html>
<html lang="en">

{{> head page="catalog" sort=true}}

<body>
    {{> header/header page="Agrobisnis"}}
    <main role="main" class="main">
        <div class="container">
            <div class="breadcrumb">
                {{>breadcrumb title="Agrobisnis"}}
            </div>
            <div class="row catalog">
                <div class="col-xl-3 catalogSort">
                    {{>catalog/sort type="categ"}}
                </div>
                <div class="col-xl-9">
                    <h2 class="catalogTitle">
                        hasil pencarian <span>‘tas laptop’</span>
                    </h2>
                    <div class="catalogList">
                        <div class="productWrap row">
                            {{>component/product-item total=8 index_start=0 index_end=8 col="col-6 col-md-3 col-xl-3"}}
                            {{>component/product-item total=8 index_start=4 index_end=12 col="col-6 col-md-3 col-xl-3"
                            lazy=true}}
                        </div>
                        <ul class="paging">
                            {{>paging}}
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </main>

    {{>help}}
    {{>footer}}

    {{>js page="catalog"}}

</body>

</html>