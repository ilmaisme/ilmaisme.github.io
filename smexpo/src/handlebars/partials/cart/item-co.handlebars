{{#each product}}
{{#gte @index compare=../index_start}}
{{#gte @index compare=../index_end}}
{{else}}
<div class="cartItem spacing-xs">
    <div class="cartContent">
        <a href="{{link}}" class="productBoxWrap">
            <div class="productImg">
                <img src="{{image}}" alt="{{title}}" width="180" height="180">
            </div>
            <div class="productBoxContent">
                <div class="productTitle">{{title}}
                </div>
                <div class="productTitle">
                    {{variant}} <span>1 barang (250gr)</span>
                </div>
                <div class="productDisc spacing-xs">
                    <span class="priceDisc">-{{disc}}</span>
                    <span class="priceNet">{{pricenet}}</span>
                    <span class="productPrice">{{price}}</span>
                </div>
            </div>
        </a>
    </div>
</div>
{{/gte}}
{{else}}
{{/gte}}
{{/each}}